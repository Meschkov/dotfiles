<keyboard>
    <chainQuitKey>C-g</chainQuitKey>
    <!-- Keybindings for desktop switching -->

    <!-- Some Spectacle MacOSX like behaivior-->
    <!-- Half left -->
    <keybind key="W-A-Left">
      <action name="UnmaximizeFull"/>3333
      <action name="MoveResizeTo">
        <width>50%</width>
      </action>
      <action name="MaximizeVert"/>
      <action name="MoveResizeTo">
        <x>0</x>
        <y>0</y>
      </action>
    </keybind>

    <!-- Half right -->
    <keybind key="W-A-Right">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
      </action>
      <action name="MaximizeVert"/>
      <action name="MoveResizeTo">
        <x>50%</x>
        <y>0</y>
      </action>
    </keybind>

    <!-- Half top -->
    <keybind key="W-A-Up">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <height>50%</height>
        <width>100%</width>
      </action>
      <action name="MoveResizeTo">
        <x>0</x>
        <y>-50%</y>
      </action>
    </keybind>

    <!-- Half bottom -->
    <keybind key="W-A-Down">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <height>50%</height>
        <width>100%</width>
      </action>
      <action name="MoveResizeTo">
        <x>0</x>
        <y>50%</y>
      </action>
    </keybind>

    <!-- Left top -->
    <keybind key="C-A-Left">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
        <height>50%</height>
        <x>0</x>
        <y>0</y>
      </action>
    </keybind>

    <!-- Left bottom -->
    <keybind key="C-A-S-Left">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
        <height>50%</height>
        <x>0</x>
        <y>50%</y>
      </action>
    </keybind>

    <!-- Right top -->
    <keybind key="C-A-Right">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
        <height>50%</height>
        <x>50%</x>
        <y>0</y>
      </action>
    </keybind>

    <!-- Right bottom -->
    <keybind key="C-A-S-Right">
      <action name="UnmaximizeFull"/>
      <action name="MoveResizeTo">
        <width>50%</width>
        <height>50%</height>
        <x>50%</x>
        <y>50%</y>
      </action>
    </keybind>

